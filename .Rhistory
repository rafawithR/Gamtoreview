annotate(geom = "rect", ymin = -Inf, ymax = 4.5, xmin = 6, xmax =9, alpha=0.3, fill="light blue") +
geom_point(aes(months, Species, color=Family), size= 27, shape=95) +
scale_x_discrete(limits=c("January", "February", "March", "April", "May", "June", "July", "August", "September", "October","November","December"), labels=c("Jan", "Feb", "Mar", "Apr", "May", "Jun", "Jul", "Aug", "Sep", "Oct", "Nov", "Dec")) +
theme_classic(base_size = 14) + labs(x="Months") +
